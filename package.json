{
  "name": "improvplus",
  "version": "1.1.0",
  "description": "Powerful tool to help facilitate Improv for everyone.",
  "private": true,
  "scripts": {
    "start": "npm run checkdb && node ./server",
    "cleardb": "node -e \"require('./server/reset').clear()\"",
    "checkdb": "node -e \"require('./server/reset').checkForSeed()\"",
    "reset": "node -e \"require('./server/reset').resetAll()\"",
    "resetGames": "node -e \"require('./server/reset-games').resetGames()\""
  },
