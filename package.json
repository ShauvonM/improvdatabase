{
  "name": "improvplus",
  "version": "1.1.0",
  "description": "Powerful tool to help facilitate Improv for everyone.",
  "private": true,
  "scripts": {
    "start": "npm run checkItems && npm run checkGames && node ./server",
    "quickstart": "node ./server",
    "clearItems": "node -e \"require('./server/reset').clear()\"",
    "checkItems": "node -e \"require('./server/reset').checkForSeed()\"",
    "resetItems": "node -e \"require('./server/reset').resetPackages()\"",
    "resetUsers": "node -e \"require('./server/reset').resetUsers()\"",
    "clearGames": "node -e \"require('./server/reset-games').clear()\"",
    "checkGames": "node -e \"require('./server/reset-games').checkForSeed()\"",
    "resetGames": "node -e \"require('./server/reset-games').resetGames()\""
  },
  "dependencies": {
    "@angular/common": "^2.4.9",
    "@angular/compiler": "^2.4.9",
    "@angular/core": "^2.4.9",
    "@angular/forms": "^2.4.9",
    "@angular/http": "^2.4.9",
    "@angular/platform-browser": "^2.4.9",
    "@angular/platform-browser-dynamic": "^2.4.9",
    "@angular/router": "^3.4.9",
    "@angular/upgrade": "^2.4.9",
    "bcrypt": "^1.0.2",
    "bluebird": "^3.5.0",
    "body-parser": "~1.15.1",
    "cookie-parser": "~1.4.3",
    "core-js": "^2.4.1",
    "express": "~4.13.4",
    "express-handlebars": "^3.0.0",
    "jwt-simple": "~0.5.0",
    "mongoose": "^4.8.6",
    "morgan": "~1.7.0",
    "nodemailer": "~2.6.4",
    "pg": "^6.1.0",
    "reflect-metadata": "^0.1.8",
    "rxjs": "^5.1.0",
    "serve-favicon": "~2.3.0",
    "stripe": "^4.15.1",
    "systemjs": "^0.19.40",
    "underscore": "~1.8.3",
    "zone.js": "^0.7.2"
  },
  "author": "Shauvon McGill <smcgill@denyconformity.com>",
  "devDependencies": {
    "@types/core-js": "^0.9.37",
    "@types/node": "^7.0.8",
    "gulp": "~3.9.1",
    "gulp-compass": "~2.1.0",
    "gulp-copy": "0.0.2",
    "gulp-livereload": "^3.8.1",
    "gulp-plumber": "^1.1.0",
    "gulp-sass": "^2.3.2",
    "gulp-sourcemaps": "~2.2.0",
    "gulp-typescript": "^3.0.2",
    "gulp-watch": "^4.3.10",
    "typescript": "^2.2.1"
  }
}
