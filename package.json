{
  "name": "improvplus",
  "version": "1.2.1",
  "description": "Powerful tool to help facilitate Improv for everyone.",
  "private": true,
  "scripts": {
    "start": "npm run checkItems && node ./server",
    "quickstart": "node ./server",
    "checkItems": "node -e \"require('./server/reset').checkForSeed()\"",
    "reset": "node -e \"require('./server/reset').reset()\"",
    "cleanup": "node -e \"require('./server/reset').cleanup()\"",
    "build:aot": "ngc -p ./tsconfig-aot.json && rollup -c ./rollup-config.js",
    "sourcemap": "source-map-explorer ./public/dist.js"
  },
